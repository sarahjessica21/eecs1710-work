float x;
float y;
float easing = 0.05;

PImage swamp;
PImage froggy;
PImage fly;
//add images to github like the prof then chnage the loadImage quotes 

void setup() { 
  size(800, 600);
  swamp = loadImage("documents/processing/projects/swamp.jpg");
  froggy = loadImage ("documents/processing/projects/froggy1.png");
}

void draw() {
  image(swamp, 0, 0);
  image(froggy, mouseX, mouseY);
}

  
